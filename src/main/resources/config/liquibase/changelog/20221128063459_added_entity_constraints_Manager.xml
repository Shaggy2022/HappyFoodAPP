<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Manager.
    -->
    <changeSet id="20221128063459-2" author="jose">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="manager"
                                 constraintName="fk_manager__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="happy_user"/>

        <addUniqueConstraint columnNames="document_number"
                             constraintName="uk_manager"
                             tableName="manager"/>
    </changeSet>
</databaseChangeLog>
